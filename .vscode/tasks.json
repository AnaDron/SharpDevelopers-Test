{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build",
			"command": "dotnet",
			"type": "process",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"args": [
				"build",
				"${workspaceFolder}/Test/Test.csproj"
			]
		},
		{
			"label": "clean",
			"type": "shell",
			"command": "find . -iname \"bin\" -print0 | xargs -0 rm -rf; find . -iname \"obj\" -print0 | xargs -0 rm -rf",
			"problemMatcher": []
		}
	]
}